<?xml version="1.0" ?>
<sdf version="1.8">
  <world name="two_joint_arm_world">

    <!-- LIGHT -->

    <light type="directional" name="sun">
        <cast_shadows>true</cast_shadows>
        <pose>0 0 10 0 0 0</pose>
        <diffuse>0.8 0.8 0.8 1</diffuse>
        <specular>0.2 0.2 0.2 1</specular>
        <attenuation>
            <range>1000</range>
            <constant>0.9</constant>
            <linear>0.01</linear>
            <quadratic>0.001</quadratic>
        </attenuation>
        <direction>-0.5 0.1 -0.9</direction>
    </light>

                            <!--      MODELS      -->

                        <!--      Ground Plane      -->

    <!-- Ground Plane -->
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>10 10</size>
            </plane>
          </geometry>
        </collision>
        <visual name="visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>10 10</size>
            </plane>
          </geometry>

          <!-- COLOR!-->
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.5 0.5 0.5 1</specular>
          </material>
        </visual>
      </link>
    </model>

                            <!-- Robotic Arm Model -->

    
    <model name="two_joint_arm">

      <plugin
        filename="libignition-gazebo-joint-controller-system.so"
        name="ignition::gazebo::systems::JointController">
        <joint_name>joint0</joint_name>
        <topic>/two_joint_arm/joint0/pos_eff</topic>
        <joint_name>joint1</joint_name>
        <topic>/two_joint_arm/joint1/pos_eff</topic>
      </plugin>


      <plugin
        filename="libignition-gazebo-joint-state-publisher-system.so"
        name="ignition::gazebo::systems::JointStatePublisher">
      </plugin>

                                <!-- Arm Links -->

                               <!-- base link 0 -->

      <!-- Base Link -->
      <link name="base_link">

        <pose>0 0 0.025 0 0 0</pose>

        <collision name="base_collision">
          <geometry>
            <cylinder>
              <radius>0.04</radius> <!-- 8 cm diameter -->
              <length>0.05</length>
            </cylinder>
          </geometry>
        </collision>

        <visual name="base_visual">
          <geometry>
            <cylinder>
              <radius>0.04</radius>
              <length>0.05</length>
            </cylinder>
          </geometry>

          <!-- COLOR!-->
          <material>
            <ambient>1.0 0.0 0.0 1</ambient>
            <diffuse>1.0 0.0 0.0 1</diffuse>
            <specular>1.0 0.0 0.0 1</specular>
          </material>
        </visual>

        <inertial>
          <mass>100</mass>
          <inertia>
            <ixx>0.100</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.100</iyy>
            <iyz>0</iyz>
            <izz>0.08</izz>
          </inertia>
        </inertial>
      </link>

                            <!-- Motor 0 link -->

      <link name="motor0">

        <pose>0 0 0.075 0 0 0</pose> 

        <!-- COLLISION -->
        <collision name="motor0_collision">
            <geometry>
                <cylinder>
                    <radius>0.025</radius>
                    <length>0.055</length>
                </cylinder>
            </geometry>
        </collision>

        <!-- VISUAL -->
        <visual name="motor0_visual">
            <geometry>
                <cylinder>
                    <radius>0.025</radius> <!-- 5 cm diameter -->
                    <length>0.055</length> <!-- 5.5 cm height -->
                </cylinder>
            </geometry>
            <material>
                <ambient>0.0 1.0 0.0 1</ambient> <!-- Green -->
                <diffuse>0.0 1.0 0.0 1</diffuse> <!-- Green -->
                <specular>1.0 1.0 1.0 1</specular>
            </material>
        </visual>
        <inertial>
            <mass>10</mass>
            <inertia>
                <ixx>0.0001709375</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.0001709375</iyy>
                <iyz>0</iyz>
                <izz>0.0003125</izz>
              </inertia>
        </inertial>
      </link>

                          <!-- Segment 0 -->

      <link name="segment0">

        <pose relative_to="motor0">0.018 0 0 0 1.57079632679 0</pose> 

        <!-- COLLISION -->
        <collision name="segment0 collision">
            <geometry>
                <cylinder>
                    <radius>0.024</radius>
                    <length>0.025</length>
                </cylinder>
            </geometry>
        </collision>

        <!-- VISUAL -->
        <visual name="segment0 visual">
            <geometry>
                <cylinder>
                    <radius>0.024</radius> <!-- 4.8cm diameter -->
                    <length>0.025</length> <!-- 2.5 cm length -->
                </cylinder>
            </geometry>
            <material>
                <ambient>0.678 0.847 0.902 1</ambient> <!-- Light Blue -->
                <diffuse>0.678 0.847 0.902 1</diffuse>
                <specular>1.0 1.0 1.0 1</specular>
            </material>
        </visual>
        <inertial>
          <mass>0.1</mass>
          <inertia>
              <ixx>0.00013125</ixx>
              <ixy>0</ixy>
              <ixz>0</ixz>
              <iyy>0.00013125</iyy>
              <iyz>0</iyz>
              <izz>0.0000025</izz>
          </inertia>
        </inertial>
      </link>
                      

                            <!-- Motor 1 link -->

      <link name="motor1">

        <pose relative_to="segment0">0 0 0.02625 0 0 0</pose> 

        <!-- COLLISION -->
        <collision name="motor1 collision">
            <geometry>
                <cylinder>
                    <radius>0.02</radius>
                    <length>0.03</length>
                </cylinder>
            </geometry>
        </collision>

        <!-- VISUAL -->
        <visual name="motor1 visual">
            <geometry>
                <cylinder>
                    <radius>0.02</radius> <!-- 4 cm diameter -->
                    <length>0.03</length> <!-- 3 cm length -->
                </cylinder>
            </geometry>
            <material>
                <ambient>0.0 1.0 0.0 1</ambient> <!-- Green -->
                <diffuse>0.0 1.0 0.0 1</diffuse> <!-- Green -->
                <specular>1.0 1.0 1.0 1</specular>
            </material>
        </visual>
        <inertial>
            <mass>1</mass>
            <inertia>
                <ixx>0.0002291666667</ixx>
                <ixy>0</ixy>
                <ixz>0</ixz>
                <iyy>0.0002291666667</iyy>
                <iyz>0</iyz>
                <izz>0.0001</izz>
            </inertia>
        </inertial>
      </link>

                            <!-- first segment-->

      <link name="segment1">

        <pose relative_to="motor1">-0.06 0 0 0 -1.57079632679 0</pose> 

        <!-- COLLISION -->
        <collision name="segment1 collision">
            <geometry>
                <cylinder>
                    <radius>0.01</radius>
                    <length>0.15</length>
                </cylinder>
            </geometry>
        </collision>

        <!-- VISUAL -->
        <visual name="segment1 visual">
            <geometry>
                <cylinder>
                    <radius>0.01</radius> <!-- 4 cm diameter -->
                    <length>0.15</length> <!-- 10 cm height -->
                </cylinder>
            </geometry>
            <material>
                <ambient>0.678 0.847 0.902 1</ambient> <!-- Light Blue -->
                <diffuse>0.678 0.847 0.902 1</diffuse>
                <specular>1.0 1.0 1.0 1</specular>
            </material>
        </visual>
        <inertial>
          <mass>0.1</mass>
          <inertia>
              <ixx>0.00013125</ixx>
              <ixy>0</ixy>
              <ixz>0</ixz>
              <iyy>0.00013125</iyy>
              <iyz>0</iyz>
              <izz>0.0000025</izz>
          </inertia>
        </inertial>
      </link>

                        <!--      JOINTS      -->


                            <!-- joint 0  -->


      <joint name="joint0" type="revolute">
         <parent>base_link</parent>
          <child>motor0</child>
          <axis>
              <xyz>0 0 1</xyz> <!-- Z-axis rotation -->
              <limit>
                <lower>-3.14159265359</lower> <!-- -180 degrees -->
                <upper>3.14159265359</upper>  <!-- 180 degrees -->
                <effort>1e6</effort>
              </limit>
          </axis>
          <pose>0 0 -0.025 0 0 0</pose>
      </joint>
      

                            <!-- joint 1  -->


      <joint name="joint1" type="revolute">
        <parent>motor0</parent>
         <child>motor1</child>
         <axis>
             <xyz>0 0 1</xyz>
             <limit>
                <lower>-3.14159265359</lower> 
                <upper>3.14159265359</upper>  
                <effort>1e6</effort>
             </limit>
             <dynamics>
              <friction>1</friction>  <!-- N m / s-->
             </dynamics>
         </axis>
         <pose>0 0 0 0 0 0</pose>
      </joint>     

                            <!-- fixed joint 0  -->

      <joint name="fixed1" type="fixed">
        <parent>motor1</parent>
        <child>segment1</child>
        <pose>-0.057 0 0.005 0 -1.57079632679 0</pose>
      </joint>                              
    </model>
  </world>
</sdf>
